{
  "name": "discord-container-ui",
  "version": "1.0.2",
  "description": "Discord Container UI für discord.js – identisches Layout wie python-discord containers",
  "author": "@pascal07_2025",
  "homepage": "https://github.com/Inn-Studios/DiscordContainerUI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inn-Studios/DiscordContainerUI.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "discord",
    "discord.js",
    "ui",
    "container",
    "statuspage"
  ],
  "dependencies": {
    "discord.js": "^14.24.2"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./Container": {
      "types": "./dist/Container.d.ts",
      "require": "./dist/Container.js",
      "default": "./dist/Container.js"
    },
    "./package.json": "./package.json"
  },
  "funding": "https://yourhoster.at",
  "publishConfig": {
    "access": "public"
  }
}
